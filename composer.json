{
    "name": "littlefish/uberduck",
    "description": "Uberduck's Rest API for PHP",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "role": "developer",
            "name": "littlefish",
            "email": "berman82312@gmail.com",
            "homepage": "https://fish.craft.me/me"
        }
    ],
    "autoload": {
        "psr-4": {
            "littlefish\\Uberduck\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "littlefish\\Uberduck\\": "tests/"
        }
    },
    "require": {
        "psr/http-client-implementation": "*",
        "php-http/discovery": "^1.19",
        "php-http/client-common": "2.x-dev",
        "php-http/message": "1.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "11.0.x-dev",
        "nyholm/psr7": "^1.8@dev",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^11.0@dev",
        "php-http/mock-client": "1.x-dev",
        "orchestra/testbench": "9.x-dev"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "littlefish\\Uberduck\\UberduckServiceProvider"
            ]
        }
    },
    "keywords": [
        "Uberduck",
        "Uberduck API"
    ],
    "homepage": "https://github.com/berman82312/uberduck"
}
